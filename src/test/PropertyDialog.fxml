<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<fx:root type="VBox" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox fx:id="headHBox" alignment="CENTER">
         <children>
            <Label fx:id="nameLabel" text="Name" />
         </children>
      </HBox>
      <HBox fx:id="contentHBox" alignment="CENTER" />
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" />
            <Button fx:id="okButton" alignment="CENTER" mnemonicParsing="false" text="Ok" />
         </children>
      </HBox>
   </children>
</fx:root>
