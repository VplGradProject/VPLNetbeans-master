<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<fx:root type="VBox" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <GridPane prefHeight="301.0" prefWidth="361.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <TextField fx:id="fieldName" GridPane.columnIndex="1" />
            <TextField fx:id="fieldType" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <TextField fx:id="fieldValue" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <ComboBox fx:id="fieldAccessTypeComboBox" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label text="Name" />
            <Label text="Type" GridPane.rowIndex="1" />
            <Label text="Access Type" GridPane.rowIndex="2" />
            <Label text="Value" GridPane.rowIndex="3" />
         </children>
      </GridPane>
      <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="okButton" mnemonicParsing="false" text="Ok" />
            <Button fx:id="cancelButton" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Cancel" />
         </children>
      </HBox>
   </children>
</fx:root>
